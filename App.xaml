<Application x:Class="WASolarSystem.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WASolarSystem"
             xmlns:views="clr-namespace:WASolarSystem.View"
             xmlns:viewmodels="clr-namespace:WASolarSystem.ViewModel"
             >
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewmodels:SolarSystemViewModel}">
            <views:SolarSystemView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:MainWindowViewModel}">
            <views:MainWindow/>
        </DataTemplate>
    </Application.Resources>
</Application>
